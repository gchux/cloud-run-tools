{
  "params": [
    {
      "id": "async",
      "label": "run test async",
      "type": [
        "boolean"
      ],
      "default": false
    },
    {
      "id": "method",
      "label": "HTTP method",
      "type": [
        "enum"
      ],
      "values": [
        "GET",
        "PUT",
        "POST",
        "DELETE",
        "PATCH",
        "HEAD",
        "OPTIONS"
      ],
      "default": "GET"
    },
    {
      "id": "proto",
      "label": "application layer protocol",
      "type": [
        "enum"
      ],
      "values": [
        "HTTP",
        "HTTPS"
      ],
      "default": "HTTPS"
    },
    {
      "id": "host",
      "label": "IP or hostname of remote service",
      "type": [
        "string"
      ]
    },
    {
      "id": "port",
      "label": "TCP port of remote service",
      "type": [
        "number"
      ],
      "min": 1,
      "max": 65535,
      "default": 443
    },
    {
      "id": "path",
      "label": "endpoint of remote service",
      "type": [
        "string"
      ],
      "default": "/"
    },
    {
      "id": "query",
      "label": "query parameters to send",
      "type": [
        "map",
        [
          "string",
          [
            "union",
            [
              "string",
              "number"
            ]
          ]
        ]
      ]
    },
    {
      "id": "headers",
      "label": "request headers to send",
      "type": [
        "map",
        [
          "string",
          [
            "union",
            [
              "string",
              "number"
            ]
          ]
        ]
      ]
    },
    {
      "id": "payload",
      "label": "request payload to send",
      "type": [
        "text"
      ]
    },
    {
      "id": "min-latency",
      "label": "minimum response latency in milliseconds",
      "type": [
        "number"
      ],
      "min": 1,
      "max": 3600000
    },
    {
      "id": "max-latency",
      "label": "maximum response latency in milliseconds",
      "type": [
        "number"
      ],
      "min": 1,
      "max": 3600000
    },
    {
      "id": "duration",
      "label": "test duration in seconds",
      "type": [
        "number"
      ],
      "min": 10,
      "max": 3600
    },
    {
      "id": "qps",
      "label": "traffic shape for QPS mode",
      "type": [
        "array",
        [
          "tuple",
          [
            "number",
            "number",
            "number"
          ]
        ]
      ]
    },
    {
      "id": "concurrency",
      "label": "traffic shape for CONCURRENCY mode",
      "type": [
        "array",
        [
          "tuple",
          [
            "number",
            "number",
            "number",
            "number",
            "number"
          ]
        ]
      ]
    }
  ],
  "tests": [
    {
      "id": "cloud_run_qps_full",
      "name": "Cloud Run QPS (full)",
      "desc": "run a fully parametrizable load test against a Cloud Run service",
      "mode": "qps",
      "params": [
        [
          "async"
        ],
        [
          "method",
          "host",
          "path"
        ],
        [
          "query"
        ],
        [
          "headers"
        ],
        [
          "payload"
        ],
        [
          "min-latency",
          "max-latency",
          "duration"
        ],
        [
          "qps"
        ]
      ]
    },
    {
      "id": "generic_qps_full",
      "name": "Generic QPS (full)",
      "desc": "run a fully parametrizable load test against any remote host",
      "mode": "qps",
      "params": [
        [
          "async"
        ],
        [
          "proto",
          "method"
        ],
        [
          "host",
          "port",
          "path"
        ],
        [
          "query"
        ],
        [
          "headers"
        ],
        [
          "payload"
        ],
        [
          "min-latency",
          "max-latency",
          "duration"
        ],
        [
          "qps"
        ]
      ]
    }
  ]
}
