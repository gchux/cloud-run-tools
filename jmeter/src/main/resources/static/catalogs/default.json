{
  "params": {
    "async": {
      "name": "async",
      "type": "boolean",
      "default": false
    },
    "method": {
      "name": "method",
      "type": "enum",
      "values": [
        "GET",
        "POST"
      ],
      "default": "GET"
    },
    "proto": {
      "name": "proto",
      "type": "enum",
      "values": [
        "HTTP",
        "HTTPS"
      ],
      "default": "HTTPS"
    },
    "host": {
      "name": "host",
      "type": "string"
    },
    "port": {
      "name": "port",
      "type": "number",
      "min": 1,
      "max": 65535,
      "default": 443
    },
    "path": {
      "name": "path",
      "type": "string",
      "default": "/"
    },
    "query": {
      "name": "query",
      "type": [
        "map",
        "string",
        [
          "union",
          "string",
          "number"
        ]
      ]
    },
    "headers": {
      "name": "headers",
      "type": [
        "map",
        "string",
        [
          "union",
          "string",
          "number"
        ]
      ]
    },
    "qps": {
      "name": "qps",
      "type": [
        "array",
        "number"
      ]
    }
  },
  "tests": [
    {
      "id": "cloud_run_qps_full",
      "name": "Cloud Run QPS (full)",
      "desc": "run a fully parametrizable load test against a Cloud Run service",
      "mode": "qps",
      "params": [
        "qps",
        "method",
        "host",
        "path",
        "query",
        "headers",
        "payload"
      ]
    },
    {
      "id": "generic_qps_full",
      "name": "Generic QPS (full)",
      "desc": "run a fully parametrizable load test against any remote host",
      "mode": "qps",
      "params": [
        "qps",
        "proto",
        "method",
        "host",
        "port",
        "path",
        "query",
        "headers",
        "payload"
      ]
    }
  ]
}
